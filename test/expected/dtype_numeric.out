(0 rows)

-- numeric comparison
SET pg_strom.enabled = on;
EXPLAIN (costs off, verbose)
SELECT id, x = y eq, x <> y ne, x > y gt, x >= z ge, y < z lt, y <= x le
  INTO test04g
  FROM rt_numeric
 WHERE id > 0;
